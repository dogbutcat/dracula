<!doctype html>
<html lang="zh-cn">

<head>
    <meta charset="UTF-8">
    <title></title>
    <meta name="keywords" content="易途吧">
    <meta name="description" content="易途吧">
    <meta name="易途吧" content="易途吧">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="shortcut icon" href="/images/icon.ico">
    <script type="application/javascript" src="/js/verie.js"></script>
    <script type="application/javascript" src="/js/loadscript.js"></script>
    {{#if isDEV}}
    {{else}}
    <% for (var css in htmlWebpackPlugin.files.css) { if(~htmlWebpackPlugin.files.css[css].indexOf('demon')) {%>
    <link href="<%= htmlWebpackPlugin.files.css[css] %>" rel="stylesheet">
    <% }} for (var css in htmlWebpackPlugin.files.css) { if(~htmlWebpackPlugin.files.css[css].indexOf('src')) {%>
    <link href="<%= htmlWebpackPlugin.files.css[css] %>" rel="stylesheet">
    <% }} %>
    {{/if}}
    <style>
        body {
            min-width: 1200px;
        }

        html,
        body,
        div,
        span,
        applet,
        object,
        iframe,
        h1,
        h2,
        h3,
        h4,
        h5,
        h6,
        p,
        blockquote,
        pre,
        a,
        abbr,
        acronym,
        address,
        big,
        cite,
        code,
        del,
        dfn,
        em,
        font,
        img,
        ins,
        kbd,
        q,
        s,
        samp,
        small,
        strike,
        strong,
        sub,
        sup,
        tt,
        var,
        b,
        u,
        i,
        center,
        dl,
        dt,
        dd,
        ol,
        ul,
        li,
        fieldset,
        form,
        label,
        legend,
        table,
        caption,
        tbody,
        tfoot,
        thead,
        tr,
        th,
        td {
            margin: 0;
            padding: 0;
            border: 0;
            outline: 0;
            vertical-align: baseline;
            background: transparent;
            font-family: PingFangSC-Regular, "PingFangSC Regular", "Microsoft YaHei", Helvetica, Arial, sans-serif;
            -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
            -webkit-touch-callout: none;
        }

        body::-webkit-scrollbar-thumb {
            background-color: rgba(221, 221, 221, 1);
            border-radius: 4px;
        }

        body::-webkit-scrollbar {
            width: 4px;
            background-color: #fff;
        }

        .title {

            position: absolute;
            margin: auto;
            color: #666666;
            line-height: 20px;
            text-align: center;
            height: 20px;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
        }

        .root {
            min-height: 100%;
            background-color: rgba(250, 252, 253, 1);
        }
    </style>

</head>

<body>

    <div id="root" class="root">
        {{!-- <div class="title">易途吧同业分销平台</div> --}}
    </div>
    <script>
        if (!window.checkIE || (checkIE.isIE() && (checkIE.isIE6() || checkIE.isIE7() || checkIE.isIE8()))) {
            window.location.pathname = "/hintPage.html"
        }

    </script>


    <!-- react 项目入口  -->
    {{#if isDEV}}
    <script type="text/javascript" src="/project/spa.dev.js"></script>
    {{else}}
    <% for (var js in htmlWebpackPlugin.files.js) { %>
    <script type="text/javascript" src="<%= htmlWebpackPlugin.files.js[js] %>"></script>
    <% } %>
    {{/if}}

</body>

</html>
{{else}}
<!DOCTYPE html>
{{{body}}}
{{/if}}